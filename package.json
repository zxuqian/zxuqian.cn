{
  "name": "zxuqian.cn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "docusaurus start --host 0.0.0.0",
    "start-en": "yarn run start -- --locale en",
    "write-translations": "docusaurus write-translations --locale en",
    "write-heading-ids": "docusaurus write-heading-ids",
    "clear": "docusaurus clear",
    "build": "docusaurus build",
    "build-cn": "docusaurus build --locale zh-CN",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "deploy-obs": "~/obsutil rm obs://zxuqian-cn/ -r -f && ~/obsutil cp build obs://zxuqian-cn/ -r -f -flat -exclude=.DS_Store",
    "gen": "node scripts/create.mjs",
    "serve": "docusaurus serve",
    "index": "docker run -it --env-file=.env -e \"CONFIG=$(cat docsearch.json | jq -r tostring)\" algolia/docsearch-scraper"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.3",
    "@docusaurus/plugin-debug": "^2.0.0-beta.3",
    "@docusaurus/plugin-ideal-image": "^2.0.0-beta.3",
    "@docusaurus/preset-classic": "^2.0.0-beta.3",
    "@docusaurus/theme-live-codeblock": "^2.0.0-beta.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@zxuqian/react-examples": "^0.1.4",
    "classnames": "^2.2.6",
    "docusaurus2-dotenv": "^1.4.0",
    "ora": "^5.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-reveal": "^1.2.2",
    "react-spring": "^8.0.27",
    "react-transition-group": "^4.4.1",
    "rehype-katex": "^4.0.0",
    "remark-math": "^3.0.1",
    "styled-components": "^5.0.1",
    "unist-util-find": "^1.0.2",
    "unist-util-find-after": "^4.0.0",
    "unist-util-select": "^4.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "figlet": "^1.5.0",
    "inquirer": "^8.0.0",
    "isomorphic-fetch": "^3.0.0"
  }
}
